<launch>
	<!-- ========================================================== -->
	<!-- Launch everything needed for Kinefly, except the           -->
   	<!-- source image streams.                                      -->
	<!-- ========================================================== -->	

	<include file="$(find Kinefly)/launch/$(env RIG)/params_kinefly.launch"             ns="kinefly"        />
	<include file="$(find Kinefly)/launch/$(env RIG)/params_phidgetsanalog.launch" 	    ns="kinefly"        />
    <include file="$(find Kinefly)/launch/$(env RIG)/params_ledpanels.launch"           ns="kinefly"        />
    <include file="$(find Kinefly)/launch/$(env RIG)/params_environment.launch"         ns="environment"    />
    <include file="$(find Kinefly)/launch/$(env RIG)/params_learning.launch"            ns="learning"    />

	<include file="$(find Kinefly)/launch/$(env RIG)/mcdaq.launch"   	                ns="mcdaq"          />
    <include file="$(find Kinefly)/launch/$(env RIG)/ledpanels.launch"   	            ns="ledpanels"      />

	<node name="kinefly"    pkg="Kinefly"   	type="kinefly.py"      	required="true" />

	<node name="flystate2phidgetsanalog"    pkg="Kinefly"   type="flystate2phidgetsanalog.py"   ns="kinefly"  required="true" />

    <!-- node name="flystate2ledpanels"    pkg="Kinefly"   	type="flystate2ledpanels.py"    required="true" -->

</launch>